ResDllCreate(path) {
static Trg,init:=VarSetCapacity(Trg,1536,0)+Numput(TS,Trg,192),TS:=DateAdd(A_NowUTC,1970,"s")
,Src:=""
LoopParse,%Src%,XY
Mod(A_Index,2)?O:="0x" A_LoopField:NumPut("0x" A_LoopField,Trg,O)
If 0<hF:=CreateFile(0x40000000,2,0,2,&path)
WriteFile(&hF,&Trg,1536,w),CloseHandle(hF)
ErrorLevel := w=1536
}